#!/usr/bin/env bash

COMMAND=$1

RESET="\033[0m"

info() {
  printf "\r[ \033[00;34m..\033[0m ] %s " "$1"
}

logcmd() {
  printf "\033[2mâ€º %s\033[22m\n" "$1"
}

user() {
  printf "\r\033[2K[ \033[0;33m??\033[0m ] %s " "$1"
}

confirm() {
  read -n 1 -p "\r\033[2K[ \033[00;33m??\033[0m ] $1 .. Press any key to continue or CTRL+C to exit" 
}

success() {
  printf "\r\033[2K[ \033[00;32mOK\033[0m ] %s\n" "$1"
}

if [ "$COMMAND" = "git" ]; then
fi

